xterm*eightBitInput:	false
xterm*Foreground:	white
xterm*Background:	black
xterm*faceName:		Monospace:size=9

URxvt.keysym.C-0: command:\033]710;xft:Monospace:size=12\007\033]711;xft:Monospace:bold:size=12\007
URxvt.keysym.C-plus: command:\033]710;xft:Monospace:size=24\007\033]711;xft:Monospace:bold:size=24\007
URxvt.keysym.C-equal: command:\033]710;xft:Monospace:size=48\007\033]711;xft:Monospace:bold:size=48\007
URxvt.keysym.C-minus: command:\033]710;xft:Monospace:size=9\007\033]711;xft:Monospace:bold:size=9\007
URxvt*scrollTtyOutput   : false 

Rxvt*foreground:	White
Rxvt*background:	Black
Rxvt*font:		xft:Monospace:size=9.2
Rxvt*scrollBar:		false

Xft.antialias: 1
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.rgba: rgb

XTerm*VT100.Translations:    #override
! Shift<Btn1Down>:select-start()

! Shift<Btn1Motion>:select-extend()

! Shift<Btn1Up>:select-end(CLIPBOARD)

! Shift<Btn2Up>:insert-selection(CLIPBOARD)

! Shift<Btn3Down>:start-extend()

! Shift<Btn3Motion>:select-extend()

! Shift<BtnUp>: select-end(CLIPBOARD)

Emacs.font: DejaVu Sans Mono-12
Emacs.fontBackend: xft
Emacs.menuBar: off
Emacs.toolBar: -1
Emacs.verticalScrollBars: off

URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
