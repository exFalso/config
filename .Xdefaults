xterm*eightBitInput:	false
xterm*Foreground:	white
xterm*Background:	black
xterm*faceName:		Hack:size=9

URxvt.perl-ext-common: default,clipboard,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5
URxvt.iso14755: False
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.keysym.C-M-c: perl:clipboard:copy
URxvt.keysym.C-M-v: perl:clipboard:paste
URxvt.keysym.C-0: command:\033]710;xft:Hack:size=12\007\033]711;xft:Hack:bold:size=12\007
URxvt.keysym.C-plus: command:\033]710;xft:Hack:size=24\007\033]711;xft:Hack:bold:size=24\007
URxvt.keysym.C-equal: command:\033]710;xft:Hack:size=48\007\033]711;xft:Hack:bold:size=48\007
URxvt.keysym.C-minus: command:\033]710;xft:Hack:size=9\007\033]711;xft:Hack:bold:size=9\007
URxvt.keysym.C-9: command:\033]710;xft:Hack:size=7\007\033]711;xft:Hack:bold:size=7\007
URxvt*scrollTtyOutput   : false
URxvt*scrollWithBuffer: false
URxvt*scrollTtyKeypress: true

Rxvt*foreground:	White
Rxvt*background:	Black
Rxvt*font:		xft:Hack:size=9
Rxvt*scrollBar:		false

URxvt*saveLines:    30000

! Xft.antialias: 1
Xft.dpi: 192
! Xft.hinting: 1
! Xft.hintstyle: hintfull
! Xft.rgba: rgb

XTerm*VT100.Translations:    #override
! Shift<Btn1Down>:select-start()

! Shift<Btn1Motion>:select-extend()

! Shift<Btn1Up>:select-end(CLIPBOARD)

! Shift<Btn2Up>:insert-selection(CLIPBOARD)

! Shift<Btn3Down>:start-extend()

! Shift<Btn3Motion>:select-extend()

! Shift<BtnUp>: select-end(CLIPBOARD)

Emacs.font: Hack-9
Emacs.fontBackend: xft
Emacs.menuBar: off
Emacs.toolBar: -1
Emacs.verticalScrollBars: off

URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
